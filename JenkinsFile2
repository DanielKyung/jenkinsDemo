pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo "data now : $(date +%Y)-$(date +%m)-$(date +%d) $(date +%H):$(date +%M):$(date +%S)"
            }
        }
    }
    post {
        always {
            echo 'pipeline job done!!!'
        }
    }    
}
